{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "args": [
                "--server=4712"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\edgeDebugAdapter.ts",
            "outFiles": [
                "d:/vscode-edge-debug/out/**/*.js"
            ]
        },
        {
            "type": "edge",
            "request": "launch",
            "name": "Attach to server",
            "program": "${workspaceFolder}/src/taskpane/excel.ts",
            "stopOnEntry": true,
            "debugServer": 4712,
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${workspaceFolder}/src/*",
                "webpack:///./node_modules/*": "${workspaceFolder}/node_modules/*",
                "webpack:///(webpack)-dev-server/*": "${workspaceFolder}/node_modules/webpack-dev-server/*",
                "webpack:///(webpack)/*": "${workspaceFolder}/webpack/*"
            }
        },
    ]
}